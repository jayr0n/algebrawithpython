   "source": [
    "from sympy import *\n",
    "\n",
    "x,y = symbols('x y')\n",
    "\n",
    "# Equations set equal to zero\n",
    "first = 2*x + y - 1 \n",
    "second = x - 2*y + 7 \n",
    "\n",
    "# The solution\n",
    "print(linsolve([first, second], (x, y))) "
   ]
